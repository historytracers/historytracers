      {
         "id": "SECTION_prerequisites",
         "text": [
            {
               "text": "<p><span id=\"htZoomImageMsg\"></span></p><p><span id=\"htChartMsg\"></span></p>",
               "source": null,
               "date_time": [
                  {
                     "type": "gregory",
                     "year": "2011",
