               "haplogroup": null,
               "history": [
                  {
                     "text": "É mencionado na Lista dos Reis Sumérios como tendo governado por 81 anos (<htcite0>; <htcite1>).",
                     "source": [
                        {
                           "type": 1,
                           "uuid": "cb216fdf-1282-42e0-aaf4-3547b53857d7",
